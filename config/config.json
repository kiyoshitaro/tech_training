{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "quicklinks-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/quicklinks/QuicklinksWebPart.js",
          "manifest": "./src/webparts/quicklinks/QuicklinksWebPart.manifest.json"
        }
      ]
    },
    "events-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/events/EventsWebPart.js",
          "manifest": "./src/webparts/events/EventsWebPart.manifest.json"
        }
      ]
    },
    "announcement-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/announcement/AnnouncementWebPart.js",
          "manifest": "./src/webparts/announcement/AnnouncementWebPart.manifest.json"
        }
      ]
    },
    "news-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/news/NewsWebPart.js",
          "manifest": "./src/webparts/news/NewsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "QuicklinksWebPartStrings": "lib/webparts/quicklinks/loc/{locale}.js",
    "EventsWebPartStrings": "lib/webparts/events/loc/{locale}.js",
    "AnnouncementWebPartStrings": "lib/webparts/announcement/loc/{locale}.js",
    "NewsWebPartStrings": "lib/webparts/news/loc/{locale}.js"
  }
}
